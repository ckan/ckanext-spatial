    xmlns:ent="eml://ecoinformatics.org/entity-2.1.0" 
    xmlns="eml://ecoinformatics.org/spatialVector-2.1.0" 
    targetNamespace="eml://ecoinformatics.org/spatialVector-2.1.0">
